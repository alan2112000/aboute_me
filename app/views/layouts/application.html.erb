  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>


<div class="row">
  <%= render 'layouts/common/nav_bar' %>
</div>

<div class="row">
  <div class="col s3 hide-on-small-only m3 l2">
    <div class="taps-wrapper">
    <%= render 'layouts/common/sidebar' %>
    </div>
  </div>

  <div class="col s9">
    <%= yield %>
    <%= render 'layouts/common/footer' %>
  </div>
</div>

</body>
</html>
